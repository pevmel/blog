<h1> Users </h1>
<% count = 0 %>
<% @users.each do |user| %>
  <p><%= user.name %></p>
  <% count += 1 %>
<% end %>
<hr>
<p>Quantity: <%= count %></p>

<h1> Authors </h1>
<% count = 0 %>
<% @authors.each do |user| %>
  <p><%= user.name %></p>
  <% count += 1 %>
<% end %>
<hr>
<p>Quantity: <%= count %></p>
